# Server Configuration
PORT=3000
NODE_ENV=development

# Replicate API
REPLICATE_API_TOKEN=your_replicate_api_token_here

# Cloudflare R2 Storage
R2_ACCOUNT_ID=your_r2_account_id
R2_ACCESS_KEY_ID=your_r2_access_key
R2_SECRET_ACCESS_KEY=your_r2_secret_key
R2_BUCKET_NAME=your_bucket_name
R2_PUBLIC_URL=https://your-public-url.com
R2_PRESIGN_EXPIRES=3600

# Performance - Concurrency Limits
# General heavy operations
CONCURRENCY_REPLACE_BG=6
CONCURRENCY_DEFAULT_HEAVY=6

# Replicate concurrency by model type
# Light models (Real-ESRGAN, GFPGAN) - fast, can handle more concurrent requests
REPLICATE_LIGHT_CONCURRENCY=8

# Heavy models (IC-Light, FLUX) - slow, resource intensive
REPLICATE_HEAVY_CONCURRENCY=4

# Comic/Story generation (Gemini + Animagine) - very resource intensive
REPLICATE_COMIC_CONCURRENCY=2

# Performance - Image Processing
IMAGE_MAX_SIDE_PX=2048
OUTPUT_QUALITY=92

# Performance - Retry Configuration
AI_RETRIES=2
AI_RETRY_BASE_MS=800
AI_RETRY_FACTOR=2

# Performance - Timeouts
HTTP_TIMEOUT_MS=30000

# LLM Models (for story-comic feature)
GEMINI_MODEL_ID=google/gemini-2.5-flash
ANIMAGINE_MODEL_ID=cjwbw/animagine-xl-3.1:6afe2e6b27dad2d6f480b59195c221884b6acc589ff4d05ff0e5fc058690fbb9
